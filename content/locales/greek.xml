  <xsp:string id="warrantytitle" filter="'$(sku.type)' $in$ ['spare-parts']">Εγγύηση</xsp:string>
  <xsp:string id="warrantycontent" filter="'$(sku.type)' $in$ ['customer-kit']">Η <b>Περιορισμένη εγγύηση εφ' όρου ζωής</b> εγγυάται ότι εάν η μνήμη της Dell υποστεί ποτέ κάποια βλάβη, θα την αντικαταστήσουμε.</xsp:string>
  <xsp:string id="warrantycontent" filter="'$(sku.type)' $in$ ['spare-parts']">Οι ανακαινισμένες μνήμες συνοδεύονται από Εγγύηση με την υποστήριξη της Dell διάρκειας 90 ημερών.</xsp:string>
  <xsp:string id="warrantycontent3" filter="'$(sku.type)' $in$ ['customer-kit']">Η περιορισμένη εγγύηση Ενός έτους που διαθέτουμε εγγυάται ότι σε περίπτωση βλάβης των αυθεντικών ανταλλακτικών Dell, θα τα αντικαταστήσουμε.</xsp:string>

  <!--// Start of Memory F & B -->
  <xsp:string id="warrantycontent2" filter="'$(sku.type)' $in$ ['customer-kit'] $and$ '$(sku.format)' $eq$ ['^[3-4].+']">Περιλαμβάνεται εγγύηση και υποστήριξη Dell διάρκειας 1 έτους</xsp:string>
  <xsp:string id="warrantycontent2" filter="'$(sku.type)' $in$ ['customer-kit'] $and$ '$(sku.format)' $not$ $eq$ ['^([3-4]|A).+']">Η <b>Περιορισμένη εγγύηση εφ' όρου ζωής</b> εγγυάται ότι εάν η μνήμη της Dell υποστεί ποτέ κάποια βλάβη, θα την αντικαταστήσουμε.</xsp:string>
  <xsp:string id="warrantycontent2" filter="'$(sku.type)' $in$ ['customer-kit'] $and$ '$(locale.region)' $in$ ['apj'] $and$ '$(sku.format)' $eq$ ['^A.+']">Περιλαμβάνεται εγγύηση και υποστήριξη Dell διάρκειας 1 έτους</xsp:string>
  <xsp:string id="warrantycontent4" filter="'$(sku.type)' $in$ ['customer-kit'] $and$ '$(locale.region)' $in$ ['emea', 'us', 'ca', 'la', 'apj','global']">Η περιορισμένη εγγύηση Ενός έτους εφ' όρου ζωής εγγυάται ότι εάν η μνήμη της Dell υποστεί ποτέ κάποια βλάβη, θα την αντικαταστήσουμε.</xsp:string>
  <xsp:string id="warrantycontent2" filter="'$(sku.type)' $in$ ['spare-parts']">Εγγύηση με την υποστήριξη της Dell διάρκειας 90 ημερών </xsp:string>
  <!--// End of Memory F & B -->
  
  <xsp:string id="warrantydetails" filter="'$(sku.type)' $in$ ['spare-parts']">Σημειώστε ότι αυτό το προϊόν αντικατάστασης είναι ανακαινισμένο. Η Dell παρέχει εγγύηση 90 ημερών για όλα τα ανακαινισμένα προϊόντα.</xsp:string>
  <xsp:string id="warrantytechspeccontent" filter="'$(sku.type)' $in$ ['customer-kit']">Περιορισμένη εγγύηση εφ' όρου ζωής</xsp:string>
  <xsp:string id="warrantytechspeccontent" filter="'$(sku.type)' $in$ ['spare-parts']">Εγγύηση 90 ημερών</xsp:string>

  <!--// Start of Memory Techspecs -->
